{
  "name": "next-prisma-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && npx prisma migrate deploy && next build",
    "start": "next start",
    "next:build": "next build",
    "next:lint": "next lint",
    "db:migrate": "npx prisma migrate dev --name ",
    "db:edit": "npx prisma studio",
    "format": "prettier --config .prettierrc \"**/*.{ts,tsx, css,less,scss,js,json,md,yaml,yml,html}\" --write",
    "pre-commit": "lint-staged",
    "commit": " git add . && npm run pre-commit && cz --no-verify",
    "deploy": "npx prisma deploy"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.18.6",
    "@fullcalendar/common": "^5.11.2",
    "@fullcalendar/core": "^5.11.2",
    "@fullcalendar/daygrid": "^5.11.2",
    "@fullcalendar/interaction": "^5.11.2",
    "@fullcalendar/react": "^5.11.2",
    "@fullcalendar/resource-timeline": "^5.11.2",
    "@fullcalendar/timeline": "^5.11.2",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "4.2.1",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "bulma": "^0.9.4",
    "next": "12.2.4",
    "next-auth": "^4.10.3",
    "next-transpile-modules": "^9.0.0",
    "react": "18.2.0",
    "react-bulma-components": "^4.1.0",
    "react-dom": "18.2.0",
    "sass": "^1.54.4",
    "styled-components": "^5.3.5",
    "superagent": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "18.7.1",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.8",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx,css,less,scss,js,json,md,yaml,html}": "npm run format"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
